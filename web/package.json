    "rollup": "2.10.8",
    "rollup-plugin-hot": "0.0.22",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte-hot": "0.7.0",
    "rollup-plugin-terser": "5.3.0",
    "svelte": "3.22.3"
  },
  "dependencies": {
    "apollo-boost": "0.4.9",
    "graphql": "15.0.0",
    "history": "4.10.1",

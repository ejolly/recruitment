{
  "name": "empirica-recruitment",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "scripts": {
    "build": "npm run build:tailwind && rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "cross-env LIVERELOAD=1 rollup -c -w",
    "dev:nollup": "cross-env NOLLUP=1 nollup -c --hot --port 5000 --content-base ./public --history-api-fallback",
    "dev": "run-p dev:nollup watch:tailwind",
    "start": "sirv public",
    "watch:tailwind": "postcss src/style.css -o public/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/style.css -o public/index.css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.2.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.3",
    "autoprefixer": "9.8.0",
    "cross-env": "6.0.3",
    "nollup": "0.11.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.1",
    "rollup": "2.10.8",
    "rollup-plugin-hot": "0.0.22",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte-hot": "0.7.0",
    "rollup-plugin-terser": "5.3.0",
    "svelte": "3.24.0",
    "tailwindcss": "1.4.6"
  },
  "dependencies": {
    "@editorjs/editorjs": "2.18.0",
    "@tailwindcss/ui": "^0.3.1",
    "apollo-boost": "0.4.9",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "codemirror": "5.56.0",
    "dayjs": "1.8.29",
    "graphql": "15.0.0",
    "history": "4.10.1",
    "path-to-regexp": "6.1.0",
    "sirv-cli": "0.4.6",
    "subscriptions-transport-ws": "0.9.17",
    "svelte-apollo": "0.3.0",
    "tippy.js": "6.2.5"
  }
}
